<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>botanicus.coreaudio.launch</string>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/jackd</string>
      <string>-d</string>
      <string>coreaudio</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/botanicus/Desktop/Music</string>

    <key>RunAtLoad</key>
    <true />

    <key>KeepAlive</key>
    <true />

    <!-- I don't have permissions to write into /var/log. -->
    <key>StandardOutPath</key>
    <string>/tmp/botanicus.coreaudio.launch.stdout</string>
    <key>StandardErrorPath</key>
    <string>/tmp/botanicus.coreaudio.launch.stderr</string>
  </dict>
</plist>
